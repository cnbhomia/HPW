 136   
 137  *continuity equations
 138   
 139  Births(yr)$(ord(yr)>1)..           NewBorns(yr) =E= 1.1 * Pop('milkcow',yr) ;
 140   
 141  SellingBulls(yr)$(ord(yr)>1)..     BullSold(yr) =E= 0.5 * NewBorns(yr);
 142   
 143  HeiferBal1(yr)$(ord(yr)>1)..       CowCount('0',yr) =E= 0.5 * NewBorns(yr) - heifsold(yr) ;
 144   
 145  HeiferBal2(yr)$(ord(yr)>1)..       CowCount('1',yr) =E= 0.95 * CowCount('0',yr-1) ;
 146   
 147  HeiferBal3(yr)$(ord(yr)>1)..       CowCount('2',yr) =E= 0.95 * CowCount('1',yr-1) ;
 148   
 149  MilkCowBal(a,yr)$[ord(a)>3 and ord(yr)>1]..      CowCount(a,yr)=E=0.98 * CowCount(a-1,yr-1) ;
 150   
 151  populae1(cows,yr)$(ord(cows)=1) .. Pop(cows,yr) =E= CowCount('0',yr) + CowCount('1',yr) ;
 152   
 153  populae2(cows,yr)$(ord(cows)=2) .. Pop(cows,yr) =E= sum(a$[ord(a)>2 and ord(a)<13] ,CowCount(a,yr)) ;
 154   
 155  totalherd(yr) ..                   herdsize(yr) =E= sum(c, Pop(c,yr));
 156   
 157  SellCow(yr)..                      oldcow(yr) =E= CowCount('12',yr) ;
 158   
 159  *growing and grazing area calculations
 160  *grazing(yr)..                      sum(cows,Pop(cows,yr)*grazAr(cows)) =L= MaxArea;
 161   
 162  growing(yr)..                      Ar4Sb(yr) + sum(g,Ar4Gr(g,yr)) + sum(cows,Pop(cows,yr)*grazAr(cows)) =L= MaxArea;
 163   
 164  *Feed production
 165  grainproduce(yr)..                 fgrow('grain',yr) =E= sum(g, Ar4Gr(g,yr)*Ygrain(g));
 166   
 167  beetproduce(yr)..                  fgrow('beet',yr) =E= Ar4Sb(yr) * Ysbeet ;
 168   
 169  Feedneed(f,yr)..                   fneed(f,yr) =E= sum(c, Pop(c,yr)* Diet(c,f));
 170   
 171  Feedbal(f,yr)..                    fgrow(f,yr) + fbuy(f,yr) =E= fneed(f,yr) + fsold(f,yr);
 172   
 173  Labor1(yr)..                       sum(cows, Pop(cows,yr)*LabCow(cows))
 174                                                                 + sum(f, fgrow(f,yr)*LabFeed(f))  =L= 5500 + extraLabor(yr) ;
 175   
 176  Labor2(yr)..                       LaborCost(yr) =E= 4000 + extraLabor(yr) * 1.2 ;
 177   
 178  MiscCosts(yr)..                    OtherCost(yr) =E= sum(c, pop(c,yr) * MiscCow(c)) + Ar4Sb(yr)*MiscFeed('beet')
 179                                                                  +  sum(g,Ar4Gr(g,yr))*MiscFeed('grain') ;
 180   
 181  OverPopulae(yr)..                  sum(cows,Pop(cows,yr)) =L= 130 + sum(CapYr $(ord(CapYr)>1 and {ord(CapYr) <=ord(yr)}),ExtraCows(CapYr))  ;
 182   
 183  Income(yr)..                       Revenue(yr) =E= 370 * pop('milkcow',yr) + sum(f, fsold(f,yr)*SellPr(f))
 184                                                                      + 30 * bullsold(yr) + 40*heifsold(yr) +120 * oldcow(yr) ;
 185   
 186  Expenditure(yr)..                  Expense(yr) =E= LaborCost(yr) +  sum(f, fbuy(f,yr)*CostPr(f)) + OtherCost(yr);
 187   
 188  ProfitPerYr(yr)$(ord(yr)>1)..      Profit(yr) =E= Revenue(yr) - Expense(yr) - 39.71* sum(CapYr $[ord(CapYr)>1 and
 189                                                                  ord(CapYr)<= ord(yr)],ExtraCows(CapYr)) ;
 190   
 191  ObjFunc..                           NetProfit =E= sum(yr$(ord(yr)>1),Profit(yr)) - 39.71* sum(yr$(ord(yr)>1), (4 + ord(yr)-1)*ExtraCows(Yr));
 192   
