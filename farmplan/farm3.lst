  99  *initial population of cows, year Y0
 100  CowCount.fx(Age,'Y0')$[ord(Age)<13]= 10;
 101  CowCount.fx('12','Y0') = 0;
 102   
 103  *Limits and constraints
 104  herdsize.up('Y5') = 1.75 * 120 ;
 105  herdsize.lo('Y5') = 0.5 * 120 ;
 106  Ar4Gr.up(g,yr) = ArGrMx(g) ;
 107   
 108  ExtraCows.l(yr) = 10;
