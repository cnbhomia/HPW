 110  Equations
 111      Births(yr)              'calculating NewBorns each year'
 112      SellingBulls (yr)       'calculating BullSold(yr)'
 113      HeiferBal1(yr)          'calculating heifers kept in the herd after birth'
 114      HeiferBal2(yr)          'Heifers Age 0  to Age 1'
 115      HeiferBal3(yr)          'Heifers Age 1  to Age 2'
 116      MilkCowBal(Age,yr)      'Milking cows continuity, Age 3  to Age 12'
 117      Totalherd(yr)           'Herd size calculation'
 118      populae1(cows,yr)       'calculating total heifers in herd each year'
 119      populae2(cows,yr)       'calculating total milking cows in herd each year'
 120      SellCow(yr)             'Selling cows of Age 12'
 121      grazing(yr)             'land area constraint for animal roaming'
 122      growing(yr)             'land area constraint for feed growing'
 123      grainproduce(yr)        'amount of grain produced'
 124      beetproduce(yr)         'amount of sugar beet produced'
 125      FeedNeed(f,yr)          'amount of Feed required by cows'
 126      FeedBal(f,yr)           'feed balance equation'
 127      Labor1(yr)              'net labour hours calculation'
 128      Labor2(yr)              'calculating yearly labor cost'
 129      MiscCosts(yr)           'calculating Misc/other costs'
 130      OverPopulae(yr)         'calculate number fo extra cows'
 131      CapOutlay(yr)           'calculating capital outlay for extra cattle'
 132      Income(yr)              'Net income each year'
 133      Expenditure(yr)         'Net expenditure each year'
 134      ProfitPerYr(yr)         'yearly profit calculation'
 135      ObjFunc                 'profit over 5 years'
 136      ;
 137   
 138  *continuity equations
 139   
 140  Births(yr)$(ord(yr)>1)..           NewBorns(yr) =E= 1.1 * Pop('milkcow',yr) ;
 141   
 142  SellingBulls(yr)$(ord(yr)>1)..     BullSold(yr) =E= 0.5 * NewBorns(yr);
 143   
 144  HeiferBal1(yr)$(ord(yr)>1)..       CowCount('0',yr) =E= 0.5 * NewBorns(yr) - heifsold(yr) ;
 145   
 146  HeiferBal2(yr)$(ord(yr)>1)..       CowCount('1',yr) =E= 0.95 * CowCount('0',yr-1) ;
 147   
 148  HeiferBal3(yr)$(ord(yr)>1)..       CowCount('2',yr) =E= 0.95 * CowCount('1',yr-1) ;
 149   
 150  MilkCowBal(a,yr)$[ord(a)>3 and ord(yr)>1]..      CowCount(a,yr)=E=0.98 * CowCount(a-1,yr-1) ;
 151   
 152  populae1(cows,yr) ..               Pop('heifer',yr) =E= CowCount('0',yr) + CowCount('1',yr) ;
 153   
 154  populae2(cows,yr) ..               Pop('milkcow',yr) =E= sum(a$[ord(a)>2 and ord(a)<13] ,CowCount(a,yr)) ;
 155   
 156  totalherd(yr) ..                   herdsize(yr) =E= sum(c, Pop(c,yr));
 157   
 158  SellCow(yr)..                      oldcow(yr) =E= CowCount('12',yr) ;
 159   
 160  *growing and grazing area calculations
 161  grazing(yr)..                      sum(cows,Pop(cows,yr)*grazAr(cows)) =L= MaxArea;
 162   
 163  growing(yr)..                      Ar4Sb(yr) + sum(g,Ar4Gr(g,yr)) =L= MaxArea;
 164   
 165  *Feed production
 166  grainproduce(yr)..                 fgrow('grain',yr) =E= sum(g, Ar4Gr(g,yr)*Ygrain(g));
 167   
 168  beetproduce(yr)..                  fgrow('beet',yr) =E= Ar4Sb(yr) * Ysbeet ;
 169   
 170  Feedneed(f,yr)..                   fneed(f,yr) =E= sum(c, Pop(c,yr)* Diet(c,f));
 171   
 172  Feedbal(f,yr)..                    fgrow(f,yr) + fbuy(f,yr) =E= fneed(f,yr) + fsold(f,yr);
 173   
 174  Labor1(yr)..                       sum(cows, Pop(cows,yr)*LabCow(cows))
 175                                                                 + sum(f, fgrow(f,yr)*LabFeed(f))  =L= 5500 + extraLabor(yr) ;
 176   
 177  Labor2(yr)..                       LaborCost(yr) =E= 4000 + extraLabor(yr) * 1.2 ;
 178   
 179  MiscCosts(yr)..                    OtherCost(yr) =E= sum(c, pop(c,yr) * MiscCow(c)) + Ar4Sb(yr)*MiscFeed('beet')
 180                                                                  +  sum(g,Ar4Gr(g,yr))*MiscFeed('grain') ;
 181   
 182  OverPopulae(yr)..                  sum(cows,Pop(cows,yr)) =L= 130 + ExtraCows(yr)  ;
 183   
 184  CapOutlay(yr)..                    capital(yr) =E= ExtraCows(yr) * CapOut ;
 185   
 186  Income(yr)..                       Revenue(yr) =E= 370 * pop('milkcow',yr) + sum(f, fsold(f,yr)*SellPr(f))
 187                                                                      + 30 * bullsold(yr) + 40*heifsold(yr) +120 * oldcow(yr) ;
 188   
 189  Expenditure(yr)..                  Expense(yr) =E= LaborCost(yr) + capital(yr) +  sum(f, fbuy(f,yr)*CostPr(f)) + OtherCost(yr);
 190   
 191  ProfitPerYr(yr)$(ord(yr)>1)..      Profit(yr) =E= Revenue(yr) - Expense(yr) - 39.71* sum(CapYr $[ord(CapYr)>1 and
 192                                                                  ord(CapYr)<= ord(yr)],ExtraCows(CapYr)) ;
 193   
 194  ObjFunc..                           NetProfit =E= sum(yr$(ord(yr)>1),Profit(yr)) - 39.71* sum(yr$(ord(yr)>1), (4 + ord(yr)-1)*ExtraCows(Yr));
 195   
 196   
 197   
 198  model farm /all/;
 199  option limrow =100, limcol=20;
