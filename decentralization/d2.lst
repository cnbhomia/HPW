   2  *         *         *         *         *         *         *         *         *
      This problem is a simple decision making problem for a company relocating it's five
      departments from London to two other cities subject to constraint of maximum
      number of offices in a city. The objective is to maximize the benefits of relocation
      against increased communication costs. 
       
      Model Building in Mathematical Programming, Fifth Edition,
      H. Paul Williams, Model 12.10 : Decentralization
      Wiley Publication, 2013
  13   
  14  Sets
  15      Dept    'Departments in the company'    /A*E/
  16      City    'Cities Available'              /'Bristol', 'Brighton' , 'London'/
  17      ;
  18   
  19  Table B(City,Dept)      'Benefits from relocation, [1000 £/yr]'
  20               A      B       C       D       E
  21  Bristol     10     15      10      20       5
  22  Brighton    10     20      15      15      15
  23      ;
  24   
  25  Table C(Dept,Dept)      'Quantity of communication between departments [1000 units] '
  26      A      B       C       D       E
  27  A                 1.0     1.5
  28  B                 1.4     1.2
  29  C                                 2.0
  30  D                                 0.7
  31      ;
  32   
  33  Table D(City, City)     'Cost of Communication between cities [£/unit] '
  34              Bristol     Brighton    London
  35  Bristol           5           14        13
  36  Brighton         14            5         9
  37  London           13            9        10
  38      ;
  39   
  40  Alias(Dept,i,k),(City,j,l);
  41   
  42  Variables
  43      Locate(Dept,City)   'Moving Decision Variable'
  44      ComOpen(i,j,k,l)    'Communication between Dept i at City j to Dept k at City l'
  45      MovCost             'Cost Benefit from Moving, [1000£]'
  46      ComCost             'Cost from comunication, [1000£] '
  47      NetCost             'Net cost, [1000£]'
  48      ;
  49   
  50  Binary Variable Locate(Dept,City),ComOpen(i,j,k,l);
  51   
  52  Equations
  53      Benefits                 'Benefit Evaluation'
  54      Communication            'Communication Cost Evaluation'
  55      BinaryCon1(i,j,k,l)      'Logical Condition ComOpen(i,j,k,l) --> Locate(i,j)'
  56      BinaryCon2(i,j,k,l)      'Logical Condition ComOpen(i,j,k,l) --> Locate(k,l)'
  57      BinaryCon3(i,j,k,l)      'Logical Condition Locate(i,j) and Locate(k,l) --> ComOpen(i,j,k,l)'
  58      MaxDept(City)            'Maximum Departments allowed in each city'
  59      MaxCity(Dept)            'Maximum City for each Department'
  60      Savings                  'Net cost'
  61      ;
  62   
  63  Benefits..                  MovCost =E= sum ((i,j), Locate(i,j)* B(j,i)) ;
  64   
  65  Communication..             ComCost =E= sum((i,j,k,l), C(i,k)*D(j,l)* ComOpen(i,j,k,l));
  66   
  67  BinaryCon1(i,j,k,l) $[(ord(i)<ord(k))]..
  68                              ComOpen(i,j,k,l) - Locate(i,j) =L= 0;
  69   
  70  BinaryCon2(i,j,k,l) $[(ord(i)<ord(k))]..
  71                              ComOpen(i,j,k,l) - Locate(k,l) =L= 0;
  72   
  73  BinaryCon3(i,j,k,l) $[(ord(i)<ord(k))]..
  74                              Locate(i,j) + Locate(k,l) -  ComOpen(i,j,k,l) =L= 1;
  75   
  76  MaxDept(City)..             sum(Dept,Locate(Dept,City)) =L= 3;
  77   
  78  MaxCity(Dept)..             sum(City,Locate(Dept,City)) =E= 1;
  79   
  80  Savings..                   NetCost =E=  - MovCost + ComCost ;
  81   
  82  Model Decentre /all/;
  83   
  84  Solve Decentre using MIP minimizing NetCost;
  85   
  86  Display Locate.l, MovCost.l, ComCost.l;
  87   
  88   
