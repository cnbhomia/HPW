--- LOAD  i = 1:i
--- LOAD  xy = 2:xy
  35   
  36  x(i) = xy(i,'x');
  37  y(i) = xy(i,"y");
  38   
  39   
  40  Variables
  41      a      'coefficient a for the general curve cx^2 + bx + a'
  42      b      'coefficient b for the general curve cx^2 + bx + a'
  43      c      'coefficient c for the general curve cx^2 + bx + a'
  44      ybar(i) 'value of y from the curve fit'
  45      e(i)    'error per observation'
  46      sumE    'total abosulate error, objvar'
  47      MinMaxE 'Minimum Maximum Error'
  48      ;
  49   
  50   
  51  Equation
  52      ybarL(i)   'calculate ybar=ax + b, linear equation'
  53      error1(i)  'calculation of absolute error, ybar-y'
  54      error2(i)  'calculation of absolute error, ybar-y'
  55      TotAbsDevL  'objfunc, linear fit, minimum total absolute deviation'
  56      MiniMaxErrL(i)
  57      ;
  58   
  59   
  60  ybarL(i)..          ybar(i) =E= b*x(i) + a;
  61  error1(i)..         e(i) =G= ybar(i) - y(i);
  62  error2(i)..         e(i) =G= y(i) - ybar(i);
  63  TotAbsDevL..        sumE =E= sum(i,e(i));
  64  MiniMaxErrL(i)..    MinMaxE =G= e(i) ;
  65   
  66  model linearfit /all/ ;
  67  solve linearfit using LP min sumE ;
  68   
  69   
  70  parameter coefficients(*,*) ;
  71  coefficients('a','linear Minsum') = a.l ;
  72  coefficients('b','linear Minsum') = b.l ;
  73   
