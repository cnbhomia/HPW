  75  coefficients('a','linear MinMax') = a.l ;
  76  coefficients('b','linear MinMax') = b.l ;
  77   
  78   
  79  Equations
  80      ybarQ(i)   'calculate ybar=c.x^2 a1x + b1, linear equation'
  81      TotAbsDevQ  'objfunc, linear fit, minimum total absolute deviation'
  82      MiniMaxErrQ(i)
  83      ;
  84   
  85  ybarQ(i)..          ybar(i) =E= c*x(i)**2 + b*x(i) + a;
  86  TotAbsDevQ..        sumE =E= sum(i,e(i));
  87  MiniMaxErrQ(i)..    MinMaxE =G= e(i) ;
  88   
