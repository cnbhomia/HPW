  99   
 100  *Minimum Manpower contraints as bound
 101  Wtotal.lo(y,sk) = ManPower(y,sk) ;
 102  *Max extra manpower each year is 150. A safe upper cound on the variable Wtotal
 103  *Wtotal.up(y,sk)= ManPower(y,sk) + 150 ;
 104   
 105  *Limits on recruitment
 106  Wrecruit.lo(y,sk) = 0;
 107  Wrecruit.up(y,sk) = Reclim(sk);
 108   
 109  *Limits and constraints on retraining
 110   
 111  Wretrain.up(y,'S1','S2') = 200;
 112   
 113  Wretrain.fx(y,'S1','S3') =0;
 114   
 115  *Limits on Short Time workers
 116  Wshortt.up(y,sk) = 50;
 117   
 118  *Current values, inital year, Y0
 119  Wtotal.fx('y0',sk)   = ManPower('y0',sk) ;
 120  Wrecruit.fx('y0',sk) =0   ;
 121  Wretrain.fx('y0',sk,skp) =0   ;
 122  Wredund.fx('y0',sk)  =0   ;
 123  Wdowngrad.fx('y0',skp,sk) =0 ;
 124  Wredund.lo(y,sk) = 0;
 125   
 126   
 127  total(y,sk)$(ord(y)>1)..    Wtotal(y,sk) =E= Wtotal(y-1,sk)*(1-RateAttr(sk,sk))
 128                                                      +sum(skp ${ord(skp)<>ord(sk)}, Wretrain(y,skp,sk)*(1-RateAttr(skp,sk)))
 129                                                      -sum (skp${ord(skp) >ord(sk)}, Wretrain(y,sk,skp))
 130                                               + Wrecruit(y,sk)*(1-RecruitAttr(sk)) -Wredund(y,sk);
 131   
 132  Overmanning(y)..            sum(sk, Wextra(y,sk)) =L= 150 ;
 133   
 134  extraman(y,sk)..            Wextra(y,sk) +  0.5*Wshortt(y,sk) =E= Wtotal(y,sk) - ManPower(y,sk) ;
 135   
 136  retrainlim(y,'S3')..        Wretrain(y,'S2','S3') =L= 0.25* Wtotal(y,'S3') ;
 137   
 138  retraincosts(y)..           CostRetrain(y) =E= sum((sk,skp), RetrainC(sk,skp)*Wretrain(y,sk,skp)) ;
 139   
 140  redundancycosts(y)..        CostRedund(y) =E= sum(sk, RedundC(sk)*Wredund(y,sk));
 141   
 142  overmancosts(y)..           CostExtra(y) =E= sum(sk, OvermanC(sk)*Wextra(y,sk));
 143   
 144  shorttimecosts(y)..         CostStime(y) =E= sum(sk, ShorttC(sk)* Wshortt(y,sk)) ;
 145   
 146  totalcosts..                TotCTC =E= sum(y, CostRetrain(y) + CostRedund(y) + CostExtra(y) + CostStime(y) ) ;
 147   
 148  objfunc..                   TotRed =E= sum((y,sk), Wredund(y,sk)) ;
